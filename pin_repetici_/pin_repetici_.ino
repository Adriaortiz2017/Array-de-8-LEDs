//********** Setup ****************************************************************
void setup()
{
    
 pinMode(led1, OUTPUT);
 
  for(int i=0; i<3; i++)
  {
    digitalWrite(led1,1);
    delay(retard);
    digitalWrite(led1,0);
    delay(retard);
  }
    
  }
